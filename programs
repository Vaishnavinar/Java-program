//31   * * * * *
            * * * * *
class starP {
    public static void main(String[] args) {
        for(int r=1;r<=2;r++)
        {
            for(int c=1;c<=5;c++)
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}

