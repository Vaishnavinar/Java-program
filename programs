public class numpat {
    public static void main(String[] args)
     {
        int k;
        for(int i =1;i<=4;i++)
        {
            for(int j=1;j<=i;j++)
            {
                k=(j%2==0)? 0:1;
                System.out.print(k+" ");
            }
            System.out.println();
        }
    }
